<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yolo.myapplication">

    <application
        android:name=".AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_fullscreen"
            android:theme="@style/FullscreenTheme" />
        <activity
            android:name=".ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.Transparent" />
        <activity android:name=".PullToRefreshActivity" />
        <activity
            android:name=".bottomsheet.BottomSheetDemoActivity"
            android:label="@string/title_activity_bottom_sheet_demo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".jd.JDPageCountActivity" />
        <activity android:name=".waveview.WaveViewActivity" />
        <activity android:name=".update.UpdateActivity" />
        <activity android:name=".databinding.DataBindingActivity" />
        <activity android:name=".hook.ProxyActivity" />
        <activity android:name=".rx.RxActivity" />
        <activity android:name=".view.Main2Activity" />
        <activity android:name=".view.ViewDemoActivity"></activity>
    </application>

</manifest>